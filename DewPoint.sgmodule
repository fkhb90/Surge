#!name=露點計算器
#!desc=使用腳本在面板顯示露點計算結果
#!author=Jason

[Panel]
露點計算器 = script-name=dewpoint-panel, update-interval=0

[Script]
# 以參數傳入室內溫度 t（°C）、相對濕度 rh（%）
dewpoint-panel = type=generic,timeout=10,script-url=https://raw.githubusercontent.com/fkhb90/Surge/main/dewpoint.js,argument=t=25&rh=70&title=露點計算器
